{
  "name": "@cats-oss/ts-proto-optimize",
  "version": "0.1.0",
  "description": "It's CLI tool for optimize TypeScript protobuf type-definition file.",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib tsconfig.tsbuildinfo",
    "build": "tsc -p .",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "prepublish": "npm run build"
  },
  "bin": {
    "ts-proto-optimize": "bin/index.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cats-oss/ts-proto-optimize.git"
  },
  "author": "Cyberagent, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cats-oss/ts-proto-optimize/issues"
  },
  "homepage": "https://github.com/cats-oss/ts-proto-optimize#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/jest": "^24.0.19",
    "@types/meow": "^5.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "standard-version": {}
}
